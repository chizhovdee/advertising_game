<h1><%= I18n.t('shop.index.title') %></h1>

<div class="groups">
  <% for groupKey in @groupKeys: %>
    <div
        class="group <%= groupKey %> <%= 'current' if @currentGroupKey == groupKey %>"
        data-group-key="<%= groupKey %>"></div>
  <% end %>
</div>

<div class="list">
  <%=@safe @.renderTemplate('shop/list') %>
</div>