<h1><%= I18n.t('transport.index.title') %></h1>

<div class="groups">
  <% for groupKey in @groupKeys: %>
  <div
      class="group <%= groupKey %> <%= 'current' if @currentGroupKey == groupKey %>"
      data-group-key="<%= groupKey %>"></div>
  <% end %>
</div>

<div class="list">
  <%=@safe @.renderTemplate('transport/list') %>
</div>